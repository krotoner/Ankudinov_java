class Task2 {
    public static void main(String[] args) {
        int IntroTaskLast5= 3;
        while (IntroTaskLast5 > 0){
            if (IntroTaskLast5 == 5) {
                System.out.println("YES");
                return;
            }
            if (IntroTaskLast5 <= 9) {
                System.out.println("NO");
                return;
            }
            IntroTaskLast5 = IntroTaskLast5 % 10;
        }
    }
}
