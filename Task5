class Task5 {
    public static void main(String[] args) {
        int cat = 4;
        if (cat == 1)
            System.out.println(cat + " кот");
        else if ((cat > 1)&&(cat < 5))
            System.out.println(cat + " котa");
        else
            System.out.println(cat + " котов");
    }
}
