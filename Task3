class Task2 {
    public static void main(String[] args) {
        int IntroTaskLast5= 313;
        int first = IntroTaskLast5;
        int last = IntroTaskLast5;
        while (IntroTaskLast5 > 10){
            first = first / 10;
            last = last % 10;
            IntroTaskLast5 = IntroTaskLast5 / 10;
        }
        if (first == last)
            System.out.println("yes");
        else
            System.out.println("no");
    }
}
